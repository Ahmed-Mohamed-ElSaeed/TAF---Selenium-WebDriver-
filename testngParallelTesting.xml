<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression Suite" parallel="tests" thread-count="2">

	<test name="Chrome Regression Test">
		<parameter name="platformName" value="Windows 10" />
		<parameter name="browser" value="chrome" />
		<parameter name="browserVersion" value="89.0" />
		<classes>
			<class name="tests.UserRegistrationParallelTesting" />
		</classes>
	</test>

	<test name="Firefox Regression Test">
		<parameter name="platformName" value="Windows 10" />
		<parameter name="browser" value="firefox" />
		<parameter name="browserVersion" value="86.0" />
		<classes>
			<class name="tests.UserRegistrationParallelTesting" />
		</classes>
	</test>
</suite> 
